<body>
    <script type = "text/Javascript">
        function factorial( x ) {
            let fact = x

            for(let i =1; i<x; i++){
                 fact = fact * (x-1);
            }
            return fact
        }
        let a = 6
        let b= 3
        let ans =0
        if(a>b){
         ans = (factorial(a))/ (factorial(a-b) * factorial(b))
        }else if(a<b){
         ans = (factorial(b))/ (factorial(b-a) * factorial(a))
            
        }else{
         ans = (factorial(a-1))/ (factorial(a-1-b) * factorial(b))

        }
        console.log(ans)
    </script>
</body>
